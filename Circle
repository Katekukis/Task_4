package task_4;
import java.lang.*;

public class Circle {
    private int l;
    private int s;
    private int r;

    public Circle (int l, int s, int r){
        this.l = l;
        this.s = s;
        this.r = r;
    }

    public int getL() {
        return l;
    }

    public void setL(int l) {
        this.l = l;
    }

    public int getS() {
        return s;
    }

    public void setS(int s) {
        this.s = s;
    }

    public int getR() {
        return r;
    }

    public void setR(int r) {
        this.r = r;
    }

    @Override
    public String toString() {
        return "Circle{" +
                "l=" + l +
                ", s=" + s +
                ", r=" + r +
                '}';
    }
}
